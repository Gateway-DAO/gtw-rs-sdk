{
  "schemes": ["https"],
  "swagger": "2.0",
  "info": {
    "description": "This is Gateway API to upload files and create a easy way to integrate with Gateway Protocol.",
    "title": "Gateway API",
    "contact": {},
    "version": "1.0"
  },
  "host": "dev.api.gateway.tech",
  "basePath": "/",
  "paths": {
    "/accounts": {
      "post": {
        "description": "Create a new account",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["Accounts"],
        "summary": "Create account",
        "parameters": [
          {
            "description": "Account data",
            "name": "account",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/model.AccountCreateRequest"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created",
            "schema": {
              "$ref": "#/definitions/model.TokenResponse"
            }
          }
        }
      }
    },
    "/accounts/me": {
      "get": {
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "description": "Get my account",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["Accounts"],
        "summary": "Get my account",
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/model.MyAccountResponse"
            }
          }
        }
      }
    },
    "/accounts/{account_id}/add-funds": {
      "post": {
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "description": "Add funds to account",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["Accounts"],
        "summary": "Add funds to account",
        "parameters": [
          {
            "type": "string",
            "description": "Account ID",
            "name": "account_id",
            "in": "path",
            "required": true
          },
          {
            "description": "Account Ledger Add Request",
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/model.AccountLedgerAddRequest"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created",
            "schema": {
              "$ref": "#/definitions/model.AccountLedgerCreateResponse"
            }
          }
        }
      }
    },
    "/auth": {
      "post": {
        "description": "Authenticate account",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["Auth"],
        "summary": "Authenticate account",
        "parameters": [
          {
            "description": "model.AuthRequest",
            "name": "authRequest",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/model.AuthRequest"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/model.TokenResponse"
            }
          }
        }
      }
    },
    "/auth/message": {
      "get": {
        "description": "Generate sign message",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["Auth"],
        "summary": "Generate sign message",
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/model.MessageResponse"
            }
          }
        }
      }
    },
    "/auth/refresh-token": {
      "get": {
        "description": "Refresh token",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["Auth"],
        "summary": "Refresh token",
        "parameters": [
          {
            "type": "string",
            "description": "Bearer token",
            "name": "Authorization",
            "in": "header",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/model.TokenResponse"
            }
          }
        }
      }
    },
    "/data-models": {
      "get": {
        "description": "Get all data models",
        "produces": ["application/json"],
        "tags": ["Data models"],
        "summary": "Get data models",
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/model.DataModelDoc"
              }
            }
          }
        }
      },
      "post": {
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "description": "Create a new data model",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["Data models"],
        "summary": "Create data model",
        "parameters": [
          {
            "description": "Data Model",
            "name": "data_model",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/model.DataModelDoc"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created",
            "schema": {
              "$ref": "#/definitions/model.DataModelDoc"
            }
          }
        }
      }
    },
    "/data-models/me": {
      "get": {
        "security": [
          {
            "TokenAuth": []
          }
        ],
        "description": "Get all data models created by a user",
        "produces": ["application/json"],
        "tags": ["Data models"],
        "summary": "Get data models by user",
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/model.DataModelDoc"
              }
            }
          }
        }
      }
    },
    "/data-models/{id}": {
      "get": {
        "description": "Get a data model by its ID",
        "produces": ["application/json"],
        "tags": ["Data models"],
        "summary": "Get data model by ID",
        "parameters": [
          {
            "type": "string",
            "description": "Data Model ID",
            "name": "id",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/model.DataModelDoc"
            }
          }
        }
      },
      "put": {
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "description": "Update a data model",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["Data models"],
        "summary": "Update data model",
        "parameters": [
          {
            "type": "string",
            "description": "Data Model ID",
            "name": "id",
            "in": "path",
            "required": true
          },
          {
            "description": "Data Model",
            "name": "data_model",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/model.DataModelDoc"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/model.DataModelDoc"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "model.AccountCreateRequest": {
      "type": "object",
      "required": ["message", "signature", "username", "wallet_address"],
      "properties": {
        "message": {
          "type": "string"
        },
        "signature": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "wallet_address": {
          "type": "string"
        }
      }
    },
    "model.AccountLedgerAddRequest": {
      "type": "object",
      "required": ["amount"],
      "properties": {
        "amount": {
          "type": "number"
        }
      }
    },
    "model.AccountLedgerCreateResponse": {
      "type": "object",
      "properties": {
        "account_id": {
          "type": "integer"
        },
        "amount": {
          "type": "number"
        },
        "balance": {
          "type": "number"
        },
        "createdAt": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        },
        "transaction_type": {
          "$ref": "#/definitions/model.TransactionType"
        },
        "updatedAt": {
          "type": "string"
        }
      }
    },
    "model.AuthRequest": {
      "type": "object",
      "required": ["message", "signature", "wallet_address"],
      "properties": {
        "message": {
          "type": "string"
        },
        "signature": {
          "type": "string"
        },
        "wallet_address": {
          "type": "string"
        }
      }
    },
    "model.DataModelDoc": {
      "type": "object",
      "properties": {
        "created_at": {
          "type": "string"
        },
        "created_by": {
          "type": "string"
        },
        "deleted_at": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        },
        "schema": {
          "type": "string"
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "title": {
          "type": "string"
        },
        "updated_at": {
          "type": "string"
        }
      }
    },
    "model.MessageResponse": {
      "type": "object",
      "properties": {
        "message": {
          "type": "string"
        }
      }
    },
    "model.MyAccountResponse": {
      "type": "object",
      "properties": {
        "created_at": {
          "type": "string"
        },
        "did": {
          "type": "string"
        },
        "profile_picture": {
          "type": "string"
        },
        "updated_at": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "wallet_address": {
          "type": "string"
        }
      }
    },
    "model.TokenResponse": {
      "type": "object",
      "properties": {
        "token": {
          "type": "string"
        }
      }
    },
    "model.TransactionType": {
      "type": "string",
      "enum": ["deposit", "withdrawal", "payment", "fee_payment"],
      "x-enum-varnames": [
        "TransactionTypeDeposit",
        "TransactionTypeWithdrawal",
        "TransactionTypePayment",
        "TransactionTypeFeePayment"
      ]
    }
  },
  "securityDefinitions": {
    "BearerAuth": {
      "description": "Type \"Bearer\" followed by a space and the JWT token.",
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    }
  }
}
